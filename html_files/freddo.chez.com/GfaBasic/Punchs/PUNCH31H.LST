' GFA PUNCH 31
'
RESERVE 150000
e1=GEMDOS(72,L:32000)
e2=GEMDOS(72,L:32000)
DIM li$(200)
DEFTEXT 10,0,0,26
TEXT 90,96,"Ca bouge"
TEXT 60,126,"Demo by FrEd."
GET 30,0,270,30,ef$
FOR t=100 TO 126
  GET 90,t-30,220,t-30,li$(t-30)
  GET 60,t,260,t,li$(t)
NEXT t
BMOVE XBIOS(3),e1,32000
DO
  PUT 30,100,ef$
  t=t+0.3
  FOR nb%=0 TO 26
    PUT 90+10*COS(t+0.2*nb%),70+nb%,li$(70+nb%)
    PUT 75+20*SIN(t+0.1*nb%),100+nb%,li$(100+nb%)
  NEXT nb%
  BMOVE XBIOS(3),2,32000
  VOID XBIOS(5,L:XBIOS(3),L:e2,W:-1)
LOOP
EDIT
